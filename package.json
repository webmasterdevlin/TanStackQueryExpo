    "@tanstack/react-query-devtools": "^5.72.0",
    "@types/react": "~18.3.20",
    "@webmasterdevlin/json-server": "^1.3.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-native-react-query-devtools": "^1.3.10",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
