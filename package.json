{
  "name": "tanstackqueryexpo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --reset-cache --localhost",
    "dev": "concurrently \"npm run backend\" \"npm run start\"",
    "dev:ios": "concurrently \"expo start --ios\" \"npm run backend\"",
    "dev:android": "concurrently \"expo start --android\" \"npm run backend\"",
    "dev:web": "concurrently \"expo start --web\" \"npm run backend\"",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "backend": "json-server --delay=2000 --port=8080 json-server/db.json"
  },
  "dependencies": {
    "@dev-plugins/react-query": "~0.3.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/query-async-storage-persister": "^5.75.0",
    "@tanstack/react-query": "^5.75.2",
    "@tanstack/react-query-persist-client": "^5.75.2",
    "axios": "^1.9.0",
    "expo": "^53.0.7",
    "expo-blur": "~14.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
